{
  "crons": [
    {
      "path": "/api/send-followups",
      "schedule": "0 15 * * *"
    }
  ],
  "rewrites": [
    { "source": "/about", "destination": "/" },
    { "source": "/how-it-works", "destination": "/" },
    { "source": "/what-is-a-construction-price-audit", "destination": "/" },
    { "source": "/contact", "destination": "/" },
    { "source": "/privacy-policy", "destination": "/" },
    { "source": "/terms-of-service", "destination": "/" },
    { "source": "/request-audit", "destination": "/" },
    { "source": "/industries", "destination": "/" },
    { "source": "/industries/:slug", "destination": "/" },
    { "source": "/hvac", "destination": "/" },
    { "source": "/solar", "destination": "/" },
    { "source": "/pools", "destination": "/" },
    { "source": "/roofing", "destination": "/" },
    { "source": "/electrical", "destination": "/" },
    { "source": "/plumbing", "destination": "/" },
    { "source": "/additions-remodels", "destination": "/" },
    { "source": "/concrete-flatwork", "destination": "/" },
    { "source": "/audit-authorization", "destination": "/" },
    { "source": "/audit-authorization/success", "destination": "/" },
    { "source": "/audit/:secureId", "destination": "/" },
    { "source": "/internal/create-audit", "destination": "/" },
    { "source": "/internal/partner-reviews", "destination": "/" },
    { "source": "/partner/rate-card", "destination": "/" },
    { "source": "/partner/master-subcontractor", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/audit-authorization",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, noarchive, nosnippet" }
      ]
    },
    {
      "source": "/audit-authorization/success",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, noarchive, nosnippet" }
      ]
    },
    {
      "source": "/partner/rate-card",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, noarchive, nosnippet" }
      ]
    },
    {
      "source": "/partner/master-subcontractor",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, noarchive, nosnippet" }
      ]
    },
    {
      "source": "/internal/partner-reviews",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, noarchive, nosnippet" }
      ]
    }
  ]
}
